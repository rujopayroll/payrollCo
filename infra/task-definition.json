{
  "family": "app",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::746662389335:role/iam-ECSTaskExecutionRole-OSWKTFM1Z7UT",
  "containerDefinitions": [
    {
      "name": "payrollapi",
      "image": "746662389335.dkr.ecr.us-east-1.amazonaws.com/payroll-app:latest",
      "cpu": 256,
      "memory": 512,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 4567,
          "hostPort": 4567,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "LogsAppPayroll",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "payroll-app"
        }
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024"
}
